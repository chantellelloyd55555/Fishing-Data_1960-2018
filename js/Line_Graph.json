{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Pacific Countries Fishing Data 1980-2018",
  "width": "container",
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/chantellelloyd55555/Fishing-Data_1960-2018/main/Fishing_Data_Ring_linegraph.csv"
  },
  "mark": {"type": "line", "point": true},
  "encoding": {
    "x": {"timeUnit": "year", "field": "Year"},
    "y": {"field": "Fishing", "type": "quantitative"},
    "color": {"field": "Country", "type": "nominal"},
    "tooltip": [
      {"timeUnit": "year", "field": "Year"},
      {"field": "Fishing", "type": "quantitative"},
      {"field": "Country", "type": "nominal"}
    ]
  },
  "config": {}
}